{
"name": "Chome Extension To Block AD",
"version": "1.0",
"manifest_version": 2,
"description": "Extension to block ad.",
"browser_action": {
"default_icon": "icon.png",
"default_popup": "popup.html"
},
"permissions": [
	"webRequest",
	"webRequestBlocking",
    	"http://*/",
    	"http://*/*",
    	"https://*/",
    	"https://*/*"
],
"background": {"scripts": ["js/background.js"]}
}
